{
  "attributes": {
    "name": {
      "type": "String",
      "trim": true,
      "required": true
    },
    "description": {
      "type": "String",
      "trim": true
    },
    "apiKey": {
      "type": "String",
      "trim": true,
      "unique": true,
      "required": true,
      "writeAccess": "none"
    },
    "user": {
      "ref": "User",
      "type": "ObjectId",
      "required": true,
      "writeAccess": "none"
    },
    "requestCount": {
      "type": "Number",
      "default": 0,
      "writeAccess": "none"
    }
  }
}
