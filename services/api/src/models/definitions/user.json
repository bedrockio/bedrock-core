{
  "attributes": {
    "email": {
      "type": "String",
      "validate": "email",
      "required": true,
      "lowercase": true,
      "trim": true
    },
    "roles": [
      {
        "role": {
          "type": "String",
          "required": true
        },
        "scope": {
          "type": "String",
          "required": true
        },
        "scopeRef": {
          "type": "ObjectId",
          "ref": "Organization"
        }
      }
    ],
    "status": {
      "type": "String",
      "writeScopes": "none",
      "enum": [
        "activate",
        "invite"
      ]
    },
    "firstName": {
      "type": "String",
      "trim": true
    },
    "lastName": {
      "type": "String",
      "trim": true
    },
    "timeZone": {
      "type": "String"
    },
    "isDeveloper": {
      "type": "Boolean",
      "default": false
    },
    "hashedPassword": {
      "type": "String",
      "readScopes": "none",
      "writeScopes": "none"
    },
    "loginAttempts": {
      "type": "Number",
      "default": 0,
      "readScopes": "none",
      "writeScopes": "none"
    },
    "lastLoginAttemptAt": {
      "type": "Date",
      "readScopes": "none",
      "writeScopes": "none"
    },
    "accessConfirmedAt": {
      "type": "Date",
      "writeScopes": "none"
    },
    "authTokenId": {
      "type": "String",
      "readScopes": "none",
      "writeScopes": "none"
    },
    "tempTokenId": {
      "type": "String",
      "readScopes": "none",
      "writeScopes": "none"
    },
    "mfaBackupCodes": [
      {
        "type": "String",
        "readScopes": "none",
        "writeScopes": "none"
      }
    ],
    "mfaMethod": {
      "type": "String",
      "enum": [
        "sms",
        "otp"
      ]
    },
    "mfaSecret": {
      "type": "String",
      "readScopes": "none",
      "writeScopes": "none"
    },
    "mfaPhoneNumber": {
      "type": "String",
      "readScopes": "none",
      "writeScopes": "none"
    }
  },
  "search": [
    "firstName",
    "lastName",
    "email"
  ]
}
